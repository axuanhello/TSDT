[Unit]
Description=Gunicorn server for SITENAME                 
[Service]                                                                            
Restart=on-failure 
User=ubuntu 
WorkingDirectory=/home/ubuntu/sites/SITENAME/source  ExecStart=/home/ubuntu/sites/SITENMAE/virtualenv/bin/gunicorn --bind unix:/tmp/SITENAME.socket notes.wsgi:application                                                                                                                                
[Install]                                                                            
WantedBy=multi-user.target